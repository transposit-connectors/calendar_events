{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/google_calendar:v1.*.*",
        "alias": "google_calendar"
      }, {
        "name": "transposit/shopify:v2.*.*",
        "alias": "shopify"
      }],
      "operations": [{
        "id": "get_calendar_events",
        "tql": {
          "parameters": [{
            "name": "calendarId",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM google_calendar.get_calendar_events\n  WHERE calendarId=@calendarId\n  AND timeMin = (select today from this.today_date)\n  LIMIT 5"
        },
        "access": {
          "requireUser": true
        }
      }, {
        "id": "today_date",
        "function": {
          "js": "today_date.js"
        }
      }],
      "dataConnections": [{
        "ref": "google_calendar.raw",
        "requireConsumerSignin": true
      }],
      "javascriptAPIVersion": "3"
    },
    "description": "A simple example demonstrating the use of a Transposit operation to get events from a Google calendar."
  }
}